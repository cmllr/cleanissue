{
"name":"GitHub +1 remover",
"homepage_url":"https://github.com/squarerootfury/hotissue/",
"description":"Removes content-empty '+1' comments from issues on GitHub.com",
"version":"1",
"manifest_version":2,
"content_scripts": [
    {
      "matches": ["https://github.com/*"],
      "js": ["jquery-2.2.1.min.js","hotissue.js"]
    }
  ],
  "icons": {
	   "16": "icon16.png",
	   "48": "icon48.png"
   }
}