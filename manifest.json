{
"name":"Cleanissue",
"homepage_url":"https://github.com/squarerootfury/cleanissue/",
"description":"Removes content-empty '+1' comments from issues on GitHub.com",
"version":"1.0",
"manifest_version":2,
"content_scripts": [
    {
      "matches": ["https://github.com/*"],
      "js": ["js/jquery-2.2.1.min.js","js/cleanissue.js"]
    }
  ],
  "icons": {
	   "16": "img/icon16.png",
	   "48": "img/icon48.png"
   }
}